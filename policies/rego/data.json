  "constitutional_constraints": {
    "hash": "cdd01ef066bc6cf2",
    "version": "2.0.0",
    "enforcement_level": "strict",
    "audit_required": true,
    "immutable_fields": [
      "constitutional_hash",
      "message_id",
      "created_at"
    ],
    "validation_rules": {
      "require_sender_id": true,
      "require_message_type": true,
      "require_timestamps": true,
      "validate_hash": true,
      "enforce_tenant_isolation": true,
      "enforce_priority_limits": true
    }
  },
  "deliberation_config": {
    "impact_threshold": 0.8,
    "high_impact_threshold": 0.9,
    "critical_impact_threshold": 0.95,
    "default_timeout_seconds": 300,
    "extended_timeout_seconds": 600,
    "fast_lane_timeout_seconds": 30,
    "require_human_review_threshold": 0.9,
    "require_multi_agent_vote_threshold": 0.95
  },
